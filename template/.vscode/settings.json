{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Atom One Dark",
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.colors": ["rgba(255,255,64,0.07)", "rgba(127,255,127,0.07)", "rgba(255,127,255,0.07)", "rgba(79,236,236,0.07)"],
  "indentRainbow.errorColor": "rgba(255, 0, 0, 0.3)",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.guides.indentation": true,
  "git.autofetch": true,
  "json.schemas": [],
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/node_modules": true
  },
  "material-icon-theme.files.associations": {
    "*.schema.ts": "Yaml"
  },
  // https://github.com/material-extensions/vscode-material-icon-theme
  "material-icon-theme.folders.associations": {
    "coupa": "Connection",
    "success-factor": "Connection",
    "service-layer": "Connection",
    "biz-exception": "Error",
    "domain": "Webpack",
    "error-handling": "Guard",
    "public-api": "Api",
    "operation-result": "ContenT",
    "schedules": "Temp",
    "jobs": "Queue",
    "schemas": "Interface"
    //  "routes": "Connection"
  },
  "terminal.integrated.scrollback": 5000
}
